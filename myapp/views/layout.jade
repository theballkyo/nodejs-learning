doctype html
html
  head
    title= title
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    <!-- Bootstrap 3.3.5 -->
    link(rel='stylesheet', href='/bootstrap/css/bootstrap.min.css')
    <!-- Font Awesome -->
    link(rel='stylesheet', href='https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css')
    <!-- Ionicons -->
    link(rel='stylesheet', href='https://code.ionicframework.com/ionicons/2.0.1/css/ionicons.min.css')
    <!-- jvectormap -->
    link(rel='stylesheet', href='/plugins/jvectormap/jquery-jvectormap-1.2.2.css')
    <!-- Theme style -->
    link(rel='stylesheet', href='/dist/css/AdminLTE.min.css')
    <!-- AdminLTE Skins. Choose a skin from the css/skins folder instead of downloading all of them to reduce the load. -->
    link(rel='stylesheet', href='/dist/css/skins/_all-skins.min.css')

    link(rel='stylesheet', href='/stylesheets/mystyle.css')

  body.hold-transition.skin-blue.sidebar-mini
    .warpper
      header.main-header
        a(href='/').logo
          span.logo-mini <b>A</b>LT
          <!-- logo for regular state and mobile devices -->
          span.log-lg <b>Admin</b>LTE
        nav.navbar.navbar-static-top(role='navigation')
          // Navbar Right Menu
          .navbar-custom-menu
            ul.nav.navbar-nav
              // Messages: style can be found in dropdown.less
              li.dropdown.messages-menu
                a.dropdown-toggle(href='#', data-toggle='dropdown')
                  i.fa.fa-envelope-o
                  span.label.label-success 4
                ul.dropdown-menu
                  li.header You have 4 messages
                  li
                    // inner menu: contains the actual data
                    ul.menu
                      li
                        // start message
                        a(href='#')
                          .pull-left
                            img.img-circle(src='/dist/img/user2-160x160.jpg', alt='User Image')
                          h4
                            | Support Team
                            small
                              i.fa.fa-clock-o
                              |  5 mins
                          p Why not buy a new awesome theme?
                      // end message
                      li
                        a(href='#')
                          .pull-left
                            img.img-circle(src='/dist/img/user3-128x128.jpg', alt='User Image')
                          h4
                            | AdminLTE Design Team
                            small
                              i.fa.fa-clock-o
                              |  2 hours
                          p Why not buy a new awesome theme?
                      li
                        a(href='#')
                          .pull-left
                            img.img-circle(src='/dist/img/user4-128x128.jpg', alt='User Image')
                          h4
                            | Developers
                            small
                              i.fa.fa-clock-o
                              |  Today
                          p Why not buy a new awesome theme?
                      li
                        a(href='#')
                          .pull-left
                            img.img-circle(src='/dist/img/user3-128x128.jpg', alt='User Image')
                          h4
                            | Sales Department
                            small
                              i.fa.fa-clock-o
                              |  Yesterday
                          p Why not buy a new awesome theme?
                      li
                        a(href='#')
                          .pull-left
                            img.img-circle(src='/dist/img/user4-128x128.jpg', alt='User Image')
                          h4
                            | Reviewers
                            small
                              i.fa.fa-clock-o
                              |  2 days
                          p Why not buy a new awesome theme?
                  li.footer
                    a(href='#') See All Messages
              // Notifications: style can be found in dropdown.less
              li.dropdown.notifications-menu
                a.dropdown-toggle(href='#', data-toggle='dropdown')
                  i.fa.fa-bell-o
                  span.label.label-warning 10
                ul.dropdown-menu
                  li.header You have 10 notifications
                  li
                    // inner menu: contains the actual data
                    ul.menu
                      li
                        a(href='#')
                          i.fa.fa-users.text-aqua
                          |  5 new members joined today
                      li
                        a(href='#')
                          i.fa.fa-warning.text-yellow
                          |  Very long description here that may not fit into the page and may cause design problems
                      li
                        a(href='#')
                          i.fa.fa-users.text-red
                          |  5 new members joined
                      li
                        a(href='#')
                          i.fa.fa-shopping-cart.text-green
                          |  25 sales made
                      li
                        a(href='#')
                          i.fa.fa-user.text-red
                          |  You changed your username
                  li.footer
                    a(href='#') View all
              // Tasks: style can be found in dropdown.less
              li.dropdown.tasks-menu
                a.dropdown-toggle(href='#', data-toggle='dropdown')
                  i.fa.fa-flag-o
                  span.label.label-danger 9
                ul.dropdown-menu
                  li.header You have 9 tasks
                  li
                    // inner menu: contains the actual data
                    ul.menu
                      li
                        // Task item
                        a(href='#')
                          h3
                            | Design some buttons
                            small.pull-right 20%
                          .progress.xs
                            .progress-bar.progress-bar-aqua(style='width: 20%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
                              span.sr-only 20% Complete
                      // end task item
                      li
                        // Task item
                        a(href='#')
                          h3
                            | Create a nice theme
                            small.pull-right 40%
                          .progress.xs
                            .progress-bar.progress-bar-green(style='width: 40%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
                              span.sr-only 40% Complete
                      // end task item
                      li
                        // Task item
                        a(href='#')
                          h3
                            | Some task I need to do
                            small.pull-right 60%
                          .progress.xs
                            .progress-bar.progress-bar-red(style='width: 60%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
                              span.sr-only 60% Complete
                      // end task item
                      li
                        // Task item
                        a(href='#')
                          h3
                            | Make beautiful transitions
                            small.pull-right 80%
                          .progress.xs
                            .progress-bar.progress-bar-yellow(style='width: 80%', role='progressbar', aria-valuenow='20', aria-valuemin='0', aria-valuemax='100')
                              span.sr-only 80% Complete
                      // end task item
                  li.footer
                    a(href='#') View all tasks
              // User Account: style can be found in dropdown.less
              li.dropdown.user.user-menu
                a.dropdown-toggle(href='#', data-toggle='dropdown')
                  img.user-image(src='/dist/img/user2-160x160.jpg', alt='User Image')
                  span.hidden-xs Alexander Pierce
                ul.dropdown-menu
                  // User image
                  li.user-header
                    img.img-circle(src='/dist/img/user2-160x160.jpg', alt='User Image')
                    p
                      | Alexander Pierce - Web Developer
                      small Member since Nov. 2012
                  // Menu Body
                  li.user-body
                    .col-xs-4.text-center
                      a(href='#') Followers
                    .col-xs-4.text-center
                      a(href='#') Sales
                    .col-xs-4.text-center
                      a(href='#') Friends
                  // Menu Footer
                  li.user-footer
                    .pull-left
                      a.btn.btn-default.btn-flat(href='#') Profile
                    .pull-right
                      a.btn.btn-default.btn-flat(href='#') Sign out
      // Content Wrapper. Contains page content
      .content-wrapper
        // Content Header (Page header)
        section.content-header
          h1
            | SpectorShop Accounting
            small Version 0.1 Beta
          ol.breadcrumb
            li
              a(href='#')
                i.fa.fa-dashboard
                |  Home
            li.active Dashboard
        // Main content
        section.content
          // /.row
          .row
            .col-md-12
              .callout.callout-info
                block content
            // /.col
          // /.row
          // Info boxes
          .row
            .col-md-3.col-sm-6.col-xs-12
              .info-box
                span.info-box-icon.bg-aqua
                  i.ion.ion-ios-gear-outline
                .info-box-content
                  span.info-box-text CPU Traffic
                  span.info-box-number
                    | 90
                    small %
                // /.info-box-content
              // /.info-box
            // /.col
            .col-md-3.col-sm-6.col-xs-12
              .info-box
                span.info-box-icon.bg-red
                  i.fa.fa-google-plus
                .info-box-content
                  span.info-box-text Likes
                  span.info-box-number 41,410
                // /.info-box-content
              // /.info-box
            // /.col
            // fix for small devices only
            .clearfix.visible-sm-block
            .col-md-3.col-sm-6.col-xs-12
              .info-box
                span.info-box-icon.bg-green
                  i.ion.ion-ios-cart-outline
                .info-box-content
                  span.info-box-text Sales
                  span.info-box-number 760
                // /.info-box-content
              // /.info-box
            // /.col
            .col-md-3.col-sm-6.col-xs-12
              .info-box
                span.info-box-icon.bg-yellow
                  i.ion.ion-ios-people-outline
                .info-box-content
                  span.info-box-text New Members
                  span.info-box-number 2,000
                // /.info-box-content
              // /.info-box
            // /.col
        // /.content
      // /.content-wrapper

  script(src='/plugins/jQuery/jQuery-2.1.4.min.js')
  <!-- Bootstrap 3.3.5 -->
  script(src='/bootstrap/js/bootstrap.min.js')
  <!-- FastClick -->
  script(src='/plugins/fastclick/fastclick.min.js')
  <!-- AdminLTE App -->
  script(src='/dist/js/app.min.js')
  <!-- Sparkline -->
  script(src='/plugins/sparkline/jquery.sparkline.min.js')
  <!-- jvectormap -->
  script(src='/plugins/jvectormap/jquery-jvectormap-1.2.2.min.js')
  script(src='/plugins/jvectormap/jquery-jvectormap-world-mill-en.js')
  <!-- SlimScroll 1.3.0 -->
  script(src='/plugins/slimScroll/jquery.slimscroll.min.js')
  <!-- ChartJS 1.0.1 -->
  script(src='/plugins/chartjs/Chart.min.js')
  <!-- AdminLTE dashboard demo (This is only for demo purposes) -->
  script(src='/dist/js/pages/dashboard2.js')
  <!-- AdminLTE for demo purposes -->
  script(src='/dist/js/demo.js')